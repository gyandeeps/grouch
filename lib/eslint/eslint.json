{
    "env": {
        "browser": true,
        "node": false
    },
    "globals": {
        "$": true
    },
    "rules": {
        "strict": false,
        "quotes": [1, "double"],
        "dot-notation": 1,
        "eqeqeq": 2,
        "camelcase": 0,
        "eol-last": 0,
        "space-infix-ops": 1,
        "block-scoped-var": 2,
        "consistent-return": 1,
        "default-case": 1,
        "no-use-before-define": [1, "nofunc"],
        "no-array-constructor": 1,
        "no-self-compare": 2,
        "new-cap": [2, "newIsCap"],
        "radix": 1,
        "no-trailing-spaces": 0
    }
}
